{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-react-hooks"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "named-imports-order": "any",
    "ordered-imports": false,
    "jsx-no-lambda": [false],
    "member-access": [true, "no-public"],
    "react-hooks-nesting": "error"
  }
}
